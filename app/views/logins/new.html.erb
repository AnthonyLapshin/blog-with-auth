<h1>Log in</h1>
    
<%= link_to "Back to posts", posts_path %>
<br/><br/>
    
<hr/>
<%= form_with model: @user, url: logins_path do |form|%>
    <div>
        <%= form.label :email %><br/>
        <%= form.email_field :email %>
    </div>
    <div>
        <%= form.label :password %><br/>
        <%= form.password_field :password %>
    </div>
    <div>
        <br/>
        <%= form.submit "Log in" %>
    </div>
<%end%>