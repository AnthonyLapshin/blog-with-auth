<h1>Sign up</h1>
<hr/>
<%= form_with model: @user, url: registrations_path do |form|%>
    <div>
        <%= form.label :email %><br/>
        <%= form.email_field :email %>
    </div>
    <div>
        <%= form.label :password %><br/>
        <%= form.password_field :password %>
    </div>
    <div>
        <br/>
        <%= form.submit "Sign Up" %>
    </div>
<%end%>